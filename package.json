{
  "name": "draggable-list-items",
  "version": "1.0.2",
  "license": "MIT",
  "description": "A reusable drag-and-drop list component built with React, Next.js, Tailwind CSS, and dnd Kit.",
  "repository": {
    "type": "git",
    "url": "https://github.com/telishreyas10/draggable-list-items.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/styles.css"
  ],
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build:demo": "next build",
    "start": "next start",
    "lint": "next lint",
